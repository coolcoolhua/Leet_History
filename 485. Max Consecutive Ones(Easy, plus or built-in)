class Solution(object):
    def findMaxConsecutiveOnes(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        '''
        
        for i in range(1,len(nums)):
            if nums[i]==1:
                nums[i]+=nums[i-1]
        return max(nums)
        
        '''
        
        
        '''    using built-in but slow
        nums=[str(num) for num in nums ]
        strnums=''.join(nums).split('0')
        return max([len(num) for num in strnums])
        
        '''